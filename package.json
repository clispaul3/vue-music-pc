{
  "name": "vue-music-pc",
  "version": "0.1.0",
  "private": true,
  "main": "./mainprocess/main.js",
  "apiurl": "https://github.com/Binaryify/NeteaseCloudMusicApi",
  "scripts": {
    "dev:web": "vue-cli-service serve --port 8086",
    "lint": "vue-cli-service lint",
    "dev:electron": "electron .",
    "commit": "bash ./scripts/commit.sh",
    "dev:bash": "bash ./scripts/dev.sh",
    "dev:api": "PORT=8088 node ./NeteaseCloudMusicApi/app.js",
    "build:web": "vue-cli-service build",
    "build:electron": "electron-builder"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "vue": "^2.5.21"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-service": "^3.3.0",
    "babel-eslint": "^10.0.1",
    "css-loader": "^2.1.0",
    "electron": "^2.0.4",
    "electron-builder": "^20.38.4",
    "electron-packager": "^13.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "vue-template-compiler": "^2.5.21"
  },
  "build": {
    "productName": "zyb-music",
    "appId": "com.example.yourapp",
    "directories": {
      "output": "builder"
    },
    "files": [
      "dist/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "icons/logo.icns"
    },
    "win": {
      "icon": "icons/logo.ico"
    }
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
